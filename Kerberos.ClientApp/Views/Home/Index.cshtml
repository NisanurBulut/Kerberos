@model List<ProductDto>
@if (Model != null)
{
    <table class="table table-sm table-bordered">
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <h4>Authorization hatası</h4>
    <a asp-action="SignIn" asp-controller="Account"></a>
}
